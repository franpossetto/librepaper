<div class="row">
	<div class="col s3 hide-on-med-and-down" style="padding-left: 0px;" >
		<ul id= "nav-mobile" class="left side-nav fixed col s12" style="transform: translateX(0%); position: relative; margin-top: 5px; height: 100vh;" >
		    <li><a class="subheader">Perfil</a></li>
			<li><a href="/user/show/<%= session.User.id %>"><%= session.User.name + " " + session.User.lastname %></a></li>
		    <li><a href="/user/edit/<%= session.User.id %>">Editar perfil</a></li>
		    <li><a href="/notification">Notificaciones</a></li>
		    <li><div class="divider"></div></li>
		    <li><a class="subheader">Administracion</a></li>
			<li><a href="/admin/index" >Mis Catedras</a></li>
            <li><a href="/subject/new">Crear nueva catedra</a></li>
			<li><div class="divider"></div></li>
			<li><a href="/session/destroy" style="color:grey;">Cerrar sesion</a></li>
		</ul>
	</div>

	<div class="col s12 m12 l9 right">
	<%if (subject.length == 0){ %>

			<h1 class ="center-align" style="border: none; font-size:26px; font-weight:500; color:#333;""> ยก Hola <%= session.User.name %> !</h1>
			<h1 class ="center-align" style="border: none; font-size:20px; font-weight:500; color:#c5c5c5;">ยก Comienza creando una catedra !</h1>
			<div class="center-align" ><a class="center-align waves-effect waves-light btn" href="/subject/new">Crear nueva catedra</a></div>

		<% } else { %>

		 <h1 style="border: none; font-size:18px; font-weight:500; color:#333;">Mis catedras </h1>
		<% _.each(subject, function(sbj) { %>
			<div class="card">
				<a href="/admin/home/<%=sbj.id%>">
					<div class="card-image waves-effect waves-block waves-light">
						<div class="card-panel amber lighten-2" style="height:100px;">
						</div>
				    </div>
			    </a>
				<div class="card-content">
					<span class="card-title activator grey-text text-darken-4"><%=sbj.name%><i class="material-icons right">more_vert</i></span>
				</div>
				<div class="card-reveal">
					
					<span class="card-title grey-text text-darken-4"><%= sbj.name%><i class="material-icons right">close</i></span>
					<a href="/admin/home/<%=sbj.id%>">Ver catedra</a><br>
					<a href="/subject/destroy/<%=sbj.id%>">Eliminar catedra</a>


				</div>
			</div>
		<% }) %>
	<% } %>

	</div>
</div>
</div>





